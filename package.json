{
    "name": "ng-common",
    "version": "0.4.0",
    "description": "Common module to start AngularJS applications",
    "repository": {
        "type": "git",
        "url": "https://github.com/mtfranchetto/ng-common"
    },
    "dependencies": {
        "inherits": "^2.0.1",
        "jquery": "^2.1.1",
        "lodash": "^2.4.1"
    },
    "devDependencies": {
        "browserify": "^5.5.0",
        "browserify-istanbul": "^0.1.2",
        "browserify-shim": "^3.7.0",
        "debowerify": "^0.8.1",
        "gulp": "^3.8.7",
        "smild": "^0.1.1"
    },
    "scripts": {
        "install": "bower install"
    },
    "engines": {
        "node": ">=0.10.0"
    },
    "browserify": {
        "transform": [
            "browserify-shim",
            "debowerify"
        ]
    },
    "browser": {
        "angular": "./bower_components/angular/angular.js"
    },
    "browserify-shim": {
        "angular": "angular"
    },
    "smild": {
        "module": true
    }
}
